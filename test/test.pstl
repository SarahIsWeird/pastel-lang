extern print_n(value: Int32);
extern print_d(value: Float64);

func myFunction(a: Int32, b: Int32): Int32 {
    // Do some magic
    return a + b;
}

func main() {
    var test: Int32 = myFunction(400, 20);

    print_n(test);
    print_d(43.5);

    if !false {
        print_n(123);
    }

    var i: UInt8 = 0;
    while (i < 10) {
        i = i + 1;
        print_n(i to Int32);
    }
}
